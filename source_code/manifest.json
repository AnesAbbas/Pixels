{
  "name": "Pixels",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "background": {
    "service_worker": "event_h.js"
  },
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_title": "Pixels: (Clk) Blur/UnBlur Website",
    "default_icon": "128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
